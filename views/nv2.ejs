        <div class="content-body"><!-- Base style table -->
<section id="base-style">
	<div class="row">
		<div class="col-xs-12">
			<div class="card">
        <h4>Choose a file csv to upload   </h4>
        <form enctype="multipart/form-data" method="post" action="/nv2">
            <input type="file" name="file">
            <input type="submit" name="Submit">
        </form>
				<div class="card-header">
					<h4 class="card-title">Base style</h4>
          <a class="heading-elements-toggle"><i class="fa fa-ellipsis-v font-medium-3"></i></a>
          
					<div class="heading-elements">
						<ul class="list-inline mb-0">
							<li><a data-action="collapse"><i class="ft-minus"></i></a></li>
							<li><a data-action="reload"><i class="ft-rotate-cw"></i></a></li>
							<li><a data-action="expand"><i class="ft-maximize"></i></a></li>
							<li><a data-action="close"><i class="ft-x"></i></a></li>
						</ul>
					</div>
				</div>
				<div class="card-body collapse in">
					<div class="card-block card-dashboard">
						<p class="card-text">The DataTables default style file has a number of features which can be enabled based on the class name of the table. These features are.</p>
						<table class="table table-striped table-bordered base-style">
							<thead>
								<tr>
									<th>ID</th>
									<th>Name</th>
									<th>Toan</th>
                                    <th>Ly</th>
                                    <th>Hoa</th>
                                    <th>Van</th>
                                    <th>Anh</th>
                                    <th>Sinh</th>
                                    <th>Su</th>
                                    <th>Dia</th>
                                    <th>NV2</th>
                                </tr>
                            </thead>
                            <tbody>
                                    <% for (var i = 0; i < users.length; i++){ %>
                                        <tr>
                                                <td><%=users[i].ID%></td>
                                                <td><%=users[i].Name%></td>
                                                <td><%=users[i].Toan%></td>
                                                <td><%=users[i].Ly%></td>
                                                <td><%=users[i].Hoa%></td>
                                                <td><%=users[i].Van%></td>
                                                <td><%=users[i].Anh%></td>
                                                <td><%=users[i].Sinh%></td>
                                                <td><%=users[i].Su%></td>
                                                <td><%=users[i].Dia%></td>
                                                <td><%=users[i].NV1%></td>
                                        </tr>
                                    <% }%>
                                
                            </tbody>
							<tfoot>
								<tr>
                                    <th>ID</th>
									<th>Name</th>
									<th>Toan</th>
                                    <th>Ly</th>
                                    <th>Hoa</th>
                                    <th>Van</th>
                                    <th>Anh</th>
                                    <th>Sinh</th>
                                    <th>Su</th>
                                    <th>Dia</th>
                                    <th>NV2</th>
								</tr>
							</tfoot>				
						</table>				
					</div>
				</div>
			</div>
		</div>
	</div>
